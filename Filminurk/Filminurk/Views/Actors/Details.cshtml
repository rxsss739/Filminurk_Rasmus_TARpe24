@model Filminurk.Models.Actors.ActorsDetailsViewModel

<h1>Näitleja info</h1>

<div>
	<hr />
	<dl class="row">
		<div class="form-group-row">
			<label asp-for="ActorID" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(Model => Model.ActorID)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group-row">
			<label asp-for="FirstName" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(Model => Model.FirstName)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group-row">
			<label asp-for="LastName" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(Model => Model.LastName)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group-row">
			<label asp-for="NickName" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(Model => Model.NickName)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group-row">
			<label asp-for="MoviesActedFor" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(Model => Model.MoviesActedFor)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group-row">
			<label asp-for="PortraitID" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(Model => Model.PortraitID)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group-row">
			<label asp-for="ActorRating" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(Model => Model.ActorRating)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group-row">
			<label asp-for="Gender" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(Model => Model.Gender)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group-row">
			<label asp-for="MovieKnownFor" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(Model => Model.MovieKnownFor)
		</div>
	</dl>

	<hr />
	<a asp-action="Index" asp-controller="Actors" class="btn btn-success">Return to list</a>
</div>